<div>
    <h2>Asociar Monitor Responsable a Campamento</h2>
    <form action="/Practica3/asociarMonitorRCampamento" method="post">
        <label for="idMonitor">Id del monitor:</label>
        <input type="text" id="idMonitor" name="idMonitor" required>

        <label for="idCampamento">Id del campamento:</label>
        <input type="text" id="idCampamento" name="idCampamento" required>

        <button type="submit" class="big-button">Asociar</button>
    </form>
</div>